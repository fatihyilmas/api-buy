{
    "version": 2,
    "builds": [
        {
            "src": "api/create-payment.js",
            "use": "@vercel/node"
        },
        {
            "src": "*.html",
            "use": "@vercel/static"
        },
        {
            "src": "*.css",
            "use": "@vercel/static"
        },
        {
            "src": "*.js",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/api/create-payment",
            "dest": "/api/create-payment.js"
        },
        {
            "src": "/",
            "dest": "/index.html"
        },
        {
            "src": "/(.*\\.(css|js))",
            "dest": "/$1"
        }
    ]
}
